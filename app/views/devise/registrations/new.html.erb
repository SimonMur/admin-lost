<div class="registration-form">
<div class="container">
<div class="reg-header">
<h2>AGENT ACCOUNT REGISTRATION</h2>
<p>every infomation is needed</p>
</div>


<%= form_for(resource, as: resource_name, url: registration_path(resource_name)) do |f| %>
    
        <%= devise_error_messages! %>
   
  <div class="col-md-4">
  <div class="field-pic">
   <span class="btn btn-large btn-success btn-file fa fa-picture-o"> Upload a Profile Picture 
    <i class="fa fa-plus"></i>
    <%= f.file_field :picture%>
    </span>
  </div>

 
 <div class="field">
    <%= f.label :first_name %><br />
    <%= f.text_field :first_name, autofocus: true , placeholder: "First Name", class: 'login-field'%>
  </div>
   <div class="field">
    <%= f.label :second_name %><br />
    <%= f.text_field :second_name, autofocus: true, placeholder: "Second Name", class: 'login-field' %>
  </div>
   <div class="field">
    <%= f.label :last_name %><br />
    <%= f.text_field :last_name, autofocus: true, placeholder: "Last Name", class: 'login-field' %>
  </div>
   
   <div class="field">
    <%= f.label :ID_Number %><br />
    <%= f.text_field :idnumber, autofocus: true, placeholder: "National ID Number", class: 'login-field' %>
  </div>
  </div>
  <div class="col-md-4"> 
   <div class="field">
    <%= f.label :mobile_number %><br />
    <%= f.text_field :mobilenumber, autofocus: true, placeholder: "Mobile Number", class: 'login-field' %>
  </div>
 
  <div class="field">
    <%= f.label :email %><br />
    <%= f.email_field :email, autofocus: true, placeholder: "Email Address", class: 'login-field' %>
  </div>

 <div class="field">
    <%= f.label :date_of_birth%><br />
    <%= f.date_field :dateofbirth, autofocus: true, placeholder: "Date Of Birth", class: 'login-field' %>
  </div>
  
   <div class="field">
    <%= f.label :county_id %><br />

    <%= f.collection_select :county_id, County.all, :id, :name,  {prompt: "Select a county"}, { class: 'login-field'}%>
 
  </div>
  
   <div class="field">
    <%= f.label :area_id %><br />
    <%= f.collection_select :area_id, Area.all, :id, :name,  {prompt: "Select  Area"}, { class: 'login-field'}%>
  </div>
    </div>
  <div class="col-md-4">
  <div class="field">
    <%= f.label :password %>
    <% if @minimum_password_length %>
    <em>(<%= @minimum_password_length %> characters minimum)</em>
    <% end %><br />
    <%= f.password_field :password, autocomplete: "off", placeholder: "Password", class: 'login-field' %>
  </div>

  <div class="field">
    <%= f.label :password_confirmation %><br />
    <%= f.password_field :password_confirmation, autocomplete: "off", placeholder: "Confirm Password", class: 'login-field' %>
  </div>
   
     <div class="boolean-field">
      <%= f.check_box :terms, class: 'btn' %>
      <%= f.label :Accept_Terms_and_Conditions %>
    </div>
<p><%= captcha_tag %> <%= text_field_tag :captcha%></p>
</div>
 <hr>
  <div class="actions">

    <%= f.submit "Proceed with Registartion", class: 'btn btn-register' %>
  </div>

<% end %>
</div>

<div class="container" style="margin-top: 0px; font-size: 13px; color: rgba(0,0,0,0.7);">
<hr>
<div class="col-md-6">
<%- if controller_name != 'sessions' %>
  <p>Already have an account? <%= link_to "Log in", new_session_path(resource_name) %><br /></p>
<% end -%>
<%- if devise_mapping.confirmable? && controller_name != 'confirmations' %>
 <p> Didn't receive confirmation instructions? <%= link_to "Resend", new_confirmation_path(resource_name) %><br /></p>
<% end -%>
</div>
<div class="col-md-6">
<%- if devise_mapping.lockable? && resource_class.unlock_strategy_enabled?(:email) && controller_name != 'unlocks' %>
  <p>Didn't receive unlock instructions? <%= link_to "Resend", new_unlock_path(resource_name) %><br /></p>
<% end -%>
</div>
</div>

</div>